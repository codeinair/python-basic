    "# While Loop\n",
    "a=[1,2,3,4,5,6,7,8,9]\n",
    "key = 5\n",
    "found= False\n",
    "position = 0\n",
    "\n",
    "while not found and position < len(a):\n",
    "    if key == a[position]:\n",
    "        found = True\n",
    "        break\n",
    "    position =position + 1\n",
    "if found:\n",
    "    print(position)\n",
    "else:\n",
    "    print(position)"
